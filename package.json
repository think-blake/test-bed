{
  "name": "first-steps",
  "version": "0.0.1",
  "description": "Storybook starter for HTML projects",
  "main": "./dist/main.js",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production webpack --config build/config/webpack.config.babel.js",
    "build-icons": "node ./build/scripts/combine-svg.js",
    "build-storybook": "build-storybook",
    "build-tokens": "node ./build/scripts/tokens.js",
    "format": "prettier --write .",
    "lint": "npm run lint:sass && npm run lint:js && npm run format",
    "lint:js": "eslint --ext .jsx,.js src/",
    "lint:sass": "stylelint '**/*.scss' --fix",
    "prefix": "node ./build/scripts/change-prefix.js tsl",
    "generate": "plop --plopfile ./build/plop-templates/plopfile.js",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006 -s src/assets/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "storybook",
    "html"
  ],
  "author": "Think Company",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:thinkcompany/starter-ui-library-html.git"
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/register": "^7.13.16",
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.2.9",
    "@storybook/html": "^6.2.9",
    "@whitespace/storybook-addon-html": "^5.0.0",
    "autoprefixer": "^10.2.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.1.1",
    "core-js": "^3.13.0",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.0",
    "dotenv-webpack": "^7.0.2",
    "eslint": "^7.27.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.4.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "plop": "^3.0.5",
    "postcss": "^8.3.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^5.3.0",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "replace-in-file": "^6.2.0",
    "sass": "^1.34.0",
    "sass-loader": "^11.1.1",
    "storybook": "^6.2.9",
    "stream-browserify": "^3.0.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-hudochenkov": "^5.0.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "svg-sprite": "^1.5.0",
    "terser-webpack-plugin": "^5.1.2",
    "webpack": "^5.38.0",
    "webpack-cli": "^4.7.0"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.scss": "stylelint --syntax=scss --fix"
  },
  "browserslist": [
    "defaults"
  ]
}
